#include <iostream>
using namespace std;
bool isPrime(int number) {
    if (number < 2) {
        return false;
    }
    for (int i = 2; i * i <= number; i++) {
        if (number % i == 0) {
            return false; 
        }
    }
    return true; 
}

void printPrimesUpToN(int N) {
    cout << "Prime numbers up to" << N << ": ";
    for (int i = 2; i <= N; i++) {
        if (isPrime(i)) {
            cout << i << " ";
        }
    };
}

int main() {
    int N;
    cout << "Enter the number N: ";
    cin >> N;
    if (N < 2) {
        cout << "There are no primes up to " << N ;
    } else {
        printPrimesUpToN(N);
    }
    
    return 0;
}
